<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:id="@+id/lockPoprLLId"
    android:padding="1dp"
    android:layout_gravity="center"
    android:orientation="horizontal">

    <LinearLayout
        android:padding="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <GridLayout
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:columnCount="3">

            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprOneTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="1"
                    android:text="1"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprTwoTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="2"
                    android:text="2"
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprThreeTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="3"
                    android:text="3"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprfourTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="4"
                    android:text="4"
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprFiveTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="5"
                    android:text="5"
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprSixTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="6"
                    android:text="6"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprSevenTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="7"
                    android:text="7"
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprEightTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="8"
                    android:text="8"
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprNineTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="9"
                    android:text="9"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lockPoprDeleteLLId"
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:gravity="center"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprDotTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="."
                    android:text="."
                    android:gravity="center"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:background="@color/numeric_popper_background">

                <TextView
                    android:id="@+id/lockPoprZeroTVId"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/numeric_popper_text"
                    android:textSize="@dimen/numeric_popper_text_size"
                    android:tag="0"
                    android:text="0"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lockPoprSubmitLLId"
                android:layout_width="@dimen/numeric_popper_cell_dimension_width"
                android:layout_height="@dimen/numeric_popper_cell_dimension_height"
                android:gravity="center"
                android:background="@color/numeric_popper_background">

                <ImageView
                    android:layout_width="@dimen/numeric_popper_text_size"
                    android:layout_height="@dimen/numeric_popper_text_size"
                    android:background="@drawable/cross_small_black"
                    android:layout_gravity="center"/>

            </LinearLayout>

        </GridLayout>


    </LinearLayout>
</LinearLayout>