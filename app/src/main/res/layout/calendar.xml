<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/calendarPageRLId"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@color/white"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:tag="main"
    android:transitionName="content">'

    <RelativeLayout
        android:id="@+id/calendarPageLLId"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/calendarHeaderId"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/layout_header_size"
            android:background="@color/finappleTheme"
            android:elevation="@dimen/layout_header_elevation"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:onClick="goToSettings"
                android:paddingRight="15dp">

                <ImageView
                    android:id="@+id/calendarSidePaneImgId"
                    android:layout_width="@dimen/image_header_size"
                    android:layout_height="@dimen/image_header_size"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/side_pane" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/calendarDateLLId"
                android:layout_width="280dp"
                android:layout_height="fill_parent"
                android:layout_marginRight="5dp"
                android:background="@color/finappleTheme"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/calendarMonthId"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginRight="5dp"
                    android:gravity="center|right"
                    android:maxLength="9"
                    android:onClick="showDatePicker"
                    android:text="SEPTEMBER"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_header_title_size" />

                <TextView
                    android:id="@+id/calendarFullYearId"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center|right"
                    android:maxLength="4"
                    android:onClick="showDatePicker"
                    android:text="2016"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_header_title_size" />
            </LinearLayout>

            <ImageView
                android:id="@+id/calendarOptionsIVId"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/options_white" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/calendarFLID"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/calendarHeaderId"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="15dp"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:paddingLeft="3dp"
            android:paddingRight="3dp">

            <GridLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="3dp"
                android:columnCount="7"
                android:gravity="center"
                android:orientation="horizontal"
                android:rowCount="1">

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="M"
                    android:textColor="@color/navy"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="T"
                    android:textColor="@color/navy"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="W"
                    android:textColor="@color/navy"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="T"
                    android:textColor="@color/navy"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="F"
                    android:textColor="@color/navy"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="S"
                    android:textColor="@color/Orange"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="49.1dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="S"
                    android:textColor="@color/Orange"
                    android:textSize="14sp" />
            </GridLayout>

            <GridView
                android:id="@+id/calendarPageCalendarGVId"
                android:layout_width="fill_parent"
                android:layout_height="270dp"
                android:gravity="center"
                android:listSelector="#00000000"
                android:numColumns="7"
                android:scrollbars="none"></GridView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/calendarFLID"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="85dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/calendar_small_tab_outer"
                    android:orientation="horizontal"
                    android:padding="0.5dp">

                    <TextView
                        android:id="@+id/calendarSummaryTVId"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/calendar_small_tab_active_inner"
                        android:gravity="center"
                        android:onClick="onTabSelect"
                        android:tag="SELECTED"
                        android:text="SUMMARY"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_tab_small_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="85dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/calendar_small_tab_outer"
                    android:orientation="horizontal"
                    android:padding="0.5dp">

                    <TextView
                        android:id="@+id/calendarAccountsTVId"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/calendar_small_tab_inactive_inner"
                        android:gravity="center"
                        android:onClick="onTabSelect"
                        android:tag=""
                        android:text="ACCOUNTS"
                        android:textColor="@color/finappleTheme"
                        android:textSize="@dimen/text_tab_small_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="85dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/calendar_small_tab_outer"
                    android:orientation="horizontal"
                    android:padding="0.5dp">

                    <TextView
                        android:id="@+id/calendarBudgetsTVId"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/calendar_small_tab_inactive_inner"
                        android:gravity="center"
                        android:onClick="onTabSelect"
                        android:tag=""
                        android:text="BUDGETS"
                        android:textColor="@color/tab_text"
                        android:textSize="@dimen/text_tab_small_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="85dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/calendar_small_tab_outer"
                    android:orientation="horizontal"
                    android:padding="0.5dp">

                    <TextView
                        android:id="@+id/calendarSchedulesTVId"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/calendar_small_tab_inactive_inner"
                        android:gravity="center"
                        android:onClick="onTabSelect"
                        android:tag=""
                        android:text="SCHEDULES"
                        android:textColor="@color/tab_text"
                        android:textSize="12sp" />
                </LinearLayout>

            </LinearLayout>

            <android.support.v4.view.ViewPager
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/calendarTabsVPId"
                android:layout_width="match_parent"
                android:layout_height="200dp" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/calendarMultiViewTabsActiveIndIVId"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@drawable/ring_darkblue_in_darkblue" />

                <ImageView
                    android:id="@+id/calendarMultiViewActionsActiveIndIVId"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/ring_white_in_blue" />

            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>

    <include layout="@layout/calendar_fab" />
</FrameLayout>