<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/settingsParentRLId"
                android:background="@color/white">

    <LinearLayout
        android:id="@+id/settingsHeaderId"
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="@color/twitterBlue"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/settingsHeaderHomeImgId"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:background="@drawable/home"
            android:onClick="onHomeClick"/>

        <TextView
            android:id="@+id/settingsLabelTVId"
            android:layout_width="270dp"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Settings"
            android:textColor="@color/white"
            android:textSize="25sp"/>

        <TextView
            android:onClick="logoutUser"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Logout"
            android:textColor="@color/white"
            android:textSize="15sp"/>

    </LinearLayout>

    <ScrollView
        android:id="@+id/settingsListLLId"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/settingsHeaderId"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!--Manage Content-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Manage Content"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Create, Modify &amp; View"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical"
                    android:onClick="toManageContent">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/manage_content"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Categories, Accounts &amp; Spent on's"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!--Budgets & Schedules-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Budgets &amp; Schedules"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Setup Budgets &amp; Schedule Events"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:onClick="onBudget"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/budgets"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Budgets"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingLeft="5dp"
                            android:text="Budgets for Categories, Accounts &amp; Spent on's"
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/schedule"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Schedules"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingLeft="5dp"
                            android:text="Schedule Transactions &amp; Transfers"
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!--Profile-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Profile"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Update your finappl Profile"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="5dp"
                        android:onClick="showPopper"
                        android:tag="PROFILE_PERSONAL">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/personal"/>

                        <TextView
                            android:layout_width="290dp"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Personal"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/work"/>

                        <TextView
                            android:layout_width="290dp"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Work"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/contact"/>

                        <TextView
                            android:layout_width="290dp"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Contact"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <!--Notifications & Widgets-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Notifications &amp; Widgets"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Event Reminders &amp; Widgets"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/notification"/>

                        <CheckBox
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:button="@null"
                            android:checked="true"
                            android:drawableRight="?android:attr/listChoiceIndicatorMultiple"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Notifications"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/sound"/>

                        <CheckBox
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:button="@null"
                            android:checked="true"
                            android:drawableRight="?android:attr/listChoiceIndicatorMultiple"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Sound"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/widgets"/>

                        <CheckBox
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:button="@null"
                            android:checked="true"
                            android:drawableRight="?android:attr/listChoiceIndicatorMultiple"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Widget"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <!--Security-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Security"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Secure Application with a PIN"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/lock"/>

                        <CheckBox
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:button="@null"
                            android:checked="true"
                            android:drawableRight="?android:attr/listChoiceIndicatorMultiple"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Lock"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/pin"/>

                        <TextView
                            android:layout_width="290dp"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="PIN"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:text="Edit"
                            android:textColor="@color/finappleThemeDark"
                            android:textSize="14sp"/>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <!--Backup & Restore-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="2dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/finappleThemeDark"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingBottom="1dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:text="Backup &amp; Restore"
                            android:textColor="@color/primaryText"
                            android:textSize="22sp"/>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingBottom="2dp"
                            android:paddingLeft="5dp"
                            android:text="Database Backups &amp; Restorations"
                            android:textColor="@color/tertiaryText"
                            android:textSize="11sp"/>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/backup"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Backup"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingLeft="5dp"
                            android:text="Last Backup on "
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:text="12 April '15"
                            android:textColor="@color/finappleThemeDark"
                            android:textSize="10sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:layout_marginLeft="2dp"
                            android:layout_marginRight="2dp"
                            android:text="at"
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:text="12:45 PM"
                            android:textColor="@color/finappleThemeDark"
                            android:textSize="10sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/restore"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Restore"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingLeft="5dp"
                            android:text="Restore Database from an earlier point"
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                    </LinearLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:layout_margin="4dp"
                        android:background="@color/LightGrey"/>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="5dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/backup_interval"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingLeft="5dp"
                            android:text="Backup Interval"
                            android:textColor="@color/secondaryText"
                            android:textSize="19sp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="18dp"
                            android:background="@color/white"
                            android:gravity="bottom"
                            android:paddingLeft="5dp"
                            android:text="Backup Frequency"
                            android:textColor="@color/tertiaryText"
                            android:textSize="10sp"/>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <!--Security-->
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
